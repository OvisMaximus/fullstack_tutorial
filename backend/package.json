    "lint:fix": "eslint . --fix"
  },
  "author": "Laura Wetterwachs",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "supertest": "^7.1.1"
  }
}
